    "#Megen Ward\n",
    "#define a function called good\n",
    "def good():\n",
    "    ['Harry', 'Ron', 'Hermione']\n",
    "\n",
    "#define odd numbers up to 10 with a step increment of 2\n",
    "def getOdds():\n",
    "    for number in range(1,10,2):\n",
    "        yield number\n",
    "#using the yielded results, print the third odd number using a for loop\n",
    "\n",
    "count = 1\n",
    "for odd in getOdds():\n",
    "    if count == 3:\n",
    "        print(\"The third odd number is:\",odd)\n",
    "        break\n",
    "    count += 1"
